{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"rootDir": "src",
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": true,
		"outDir": "dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
