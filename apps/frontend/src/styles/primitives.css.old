:root {
  /* Colors */
  /* Pink */
  --pink-100: #FFF1F3;
  --pink-200: #FFCED6;
  --pink-300: #FFA9BA;
  --pink-400: #FF809D;
  --pink-500: #F75482;
  --pink-600: #DE376C;
  --pink-700: #C40055;
  --pink-800: #90003D;
  --pink-900: #5E0025;
  --pink-1000: #350011;

  /* Rosybrown */
  --rosybrown-100: #FFF0FC;
  --rosybrown-200: #F6D1EF;
  --rosybrown-300: #DFBBD9;
  --rosybrown-400: #C8A5C2;
  --rosybrown-500: #B290AC;
  --rosybrown-600: #9E7A98;
  --rosybrown-700: #8C5C85;
  --rosybrown-800: #72356A;
  --rosybrown-900: #55044F;
  --rosybrown-1000: #2F002B;

  /* Blue */
  --blue-100: #ECF7FF;
  --blue-200: #C2E6FF;
  --blue-300: #95D4FF;
  --blue-400: #79C1F0;
  --blue-500: #65ADDB;
  --blue-600: #4E99C8;
  --blue-700: #007FB7;
  --blue-800: #005A85;
  --blue-900: #003753;
  --blue-1000: #001B2B;

  /* Pale Blue */
  --pale-blue-100: #E9F8FF;
  --pale-blue-200: #E9F8FF;
  --pale-blue-300: #78B7D1;
  --pale-blue-400: #5694AD;
  --pale-blue-500: #33728A;
  --pale-blue-600: #00526A;
  --pale-blue-700: #00465C;
  --pale-blue-800: #003749;
  --pale-blue-900: #002836;
  --pale-blue-1000: #001C26;

  /* Black */
  --black-1000: #000000;

  /* White */
  --white-1000: #FFFFFF;

  /* Green */
  --green-100: #DEFFE6;
  --green-200: #85FFAB;
  --green-300: #6BEB96;
  --green-400: #55D683;
  --green-500: #3CC270;
  --green-600: #13AE5C;
  --green-700: #008F49;
  --green-800: #006732;
  --green-900: #003F1D;
  --green-1000: #001F0B;

  /* Gray */
  --gray-100: #F5F5F5;
  --gray-200: #E6E6E6;
  --gray-300: #D9D9D9;
  --gray-400: #B3B3B3;
  --gray-500: #757575;
  --gray-600: #444444;
  --gray-700: #383838;
  --gray-800: #2C2C2C;
  --gray-900: #1E1E1E;
  --gray-1000: #111111;

  /* Red */
  --red-100: #FFF2F0;
  --red-200: #FFD0C9;
  --red-300: #FFADA1;
  --red-400: #FF8576;
  --red-500: #FF5245;
  --red-600: #EC221F;
  --red-700: #C9000A;
  --red-800: #940005;
  --red-900: #600002;
  --red-1000: #360001;

  /* Yellow */
  --yellow-100: #FFF3E4;
  --yellow-200: #FFE5C2;
  --yellow-300: #FFD69F;
  --yellow-400: #FFC777;
  --yellow-500: #FFB643;
  --yellow-600: #F7A917;
  --yellow-700: #C98700;
  --yellow-800: #8D5E00;
  --yellow-900: #523500;
  --yellow-1000: #241500;

  /* Sizes */
  /* Space */
  --space-50: 0.125rem;
  --space-100: 0.25rem;
  --space-150: 0.375rem;
  --space-200: 0.5rem;
  --space-300: 0.75rem;
  --space-400: 1rem;
  --space-450: 1.125rem;
  --space-600: 1.5rem;
  --space-700: 1.75rem;
  --space-800: 2rem;
  --space-900: 2.25rem;
  --space-1000: 2.5rem;
  --space-1200: 3rem;
  --space-1600: 4rem;
  --space-2400: 6rem;
  --space-4000: 10rem;

  /* Radius */
  --radius-100: 0.25rem;
  --radius-200: 0.5rem;
  --radius-400: 1rem;
  --radius-9999: 999rem;

  /* Stroke */
  --stroke-25: 0.063rem;
  --radius-50: 0.125rem;
  --radius-75: 0.188rem;
  --radius-100: 0.25rem;

  /* Typography */
  /* Font */
  --font-gotham: "Gotham", sans-serif;
  --font-cal-sans: "Cal Sans", sans-serif;
  --font-comfortaa: "Comfortaa", sans-serif;
  --font-opendyslexic: "OpenDyslexic", sans-serif;
}

